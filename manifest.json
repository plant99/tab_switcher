{
  "manifest_version": 2,
  "description":"Switch tabs with just keys",
  "name": "My Cool Extension",
  "version": "1.0",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["content.js"]
	  }
	],
	"browser_action": {
		"default_icon": "532.png"
	},
	"background":{
		"scripts":["background.js"]
	},
	"permissions":[
		"tabs",
		"notifications"
	]
}
