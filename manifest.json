{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["content.js"]
	  }
	],
	"browser_action": {
		"default_icon":"532.png"
	},
	"background":{
		"scripts":["background.js"]
	},
	"permissions":[
		"tabs"
	]
}